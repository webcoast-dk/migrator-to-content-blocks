{
    "name": "webcoast/migrator-to-content-blocks",
    "description": "TYPO3 CMS extension to build content block configuration from provided input",
    "type": "typo3-cms-extension",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Thorben Nissen",
            "email": "thorben@webcoast.dk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "typo3/cms": {
            "extension-key": "migrator_to_content_blocks"
        }
    },
    "require": {
        "friendsoftypo3/content-blocks": "^1.3",
        "typo3/cms-core": "^13.4",
        "webcoast/migrator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WEBcoast\\MigratorToContentBlocks\\": "Classes"
        }
    }
}
